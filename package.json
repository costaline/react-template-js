{
  "name": "react-template-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:serve": "npm run build && npm run serve",
    "build:treemap": "env-cmd -f .env.treemap npm run build",
    "serve": "serve -s build",
    "lint:editorconfig": "editorconfig-checker",
    "lint:prettier": "prettier --ignore-unknown --check 'src/**/*' ",
    "lint:prettier:fix": "prettier --ignore-unknown --list-different --write 'src/**/*' ",
    "lint:es": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "npm run lint:es -- --fix",
    "lint:style:css": "stylelint 'src/**/*.{css,scss,sass}'",
    "lint:style:css:fix": "npm run lint:style:css -- --fix",
    "lint:style:js": "stylelint 'src/**/*.{js,jsx,tsx}'",
    "lint:style:js:fix": "npm run lint:style:js -- --fix",
    "lint:style": "npm run lint:style:css ; npm run lint:style:js",
    "lint:style:fix": "npm run lint:style:css:fix ; npm run lint:style:js:fix",
    "lint": "npm run lint:es ; npm run lint:style",
    "lint:fix": "npm run lint:es:fix ; npm run lint:style:fix",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "classnames": "^2.3.1",
    "craco-styled-jsx": "^1.0.1",
    "env-cmd": "^10.1.0",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "reseter.css": "^2.0.0",
    "sass": "^1.42.1",
    "serve": "^12.0.0",
    "styled-jsx-plugin-sass": "^1.0.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@craco/craco": "^6.1.1",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.0",
    "craco-alias": "^3.0.1",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "redux-devtools-extension": "^2.13.9",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}

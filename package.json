{
  "name": "react-template-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:serve": "npm run build && npm run serve",
    "build:treemap": "env-cmd -f .env.treemap npm run build",
    "serve": "serve -s build",
    "lint:editorconfig": "editorconfig-checker",
    "lint:prettier": "prettier --ignore-unknown --check 'src/**/*' ",
    "lint:prettier:fix": "prettier --ignore-unknown --list-different --write 'src/**/*' ",
    "lint:eslint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:stylelint": "stylelint 'src/**/*.{css,scss}'",
    "lint:stylelint:fix": "npm run lint:stylelint -- --fix",
    "lint": "npm run lint:eslint ; npm run lint:stylelint",
    "lint:fix": "npm run lint:eslint:fix ; npm run lint:stylelint:fix",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "env-cmd": "^10.1.0",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "reseter.css": "^1.1.3",
    "sass": "^1.32.11",
    "serve": "^12.0.0",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@craco/craco": "^6.1.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "craco-alias": "^2.3.0",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.3.0",
    "prop-types": "^15.7.2",
    "redux-devtools-extension": "^2.13.9",
    "sass-resources-loader": "^2.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
